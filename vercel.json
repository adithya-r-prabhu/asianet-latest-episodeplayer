{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/episodes",
            "destination": "/api/episodes"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ],
    "crons": [
        {
            "path": "/api/episodes",
            "schedule": "*/10 * * * *"
        }
    ]
}
